    content: '';
    display: block;
    position: absolute;
    top: 5px;
    bottom: 5px;
    left: 5px;
    right: 5px;
    border: 1px dashed var(--color-body-text);
    border-radius: 4px;
    opacity: 0.3;
  }
}

