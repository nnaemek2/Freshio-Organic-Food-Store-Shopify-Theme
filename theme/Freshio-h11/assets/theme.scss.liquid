/*================ Theme banner ================*/ 
.js-masonry{
  .banner-theme{
    margin-bottom:20px;
  }
}
.banner-theme{
  position:relative;
  overflow: hidden;
  img{
    width: 100%;
  }
  &>a{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 100;
    text-indent: 200%;
    white-space: nowrap;
    font-size: 0;
    opacity: 0;
  }
  figcaption{
    z-index: 1;
    div > *{
      transition-delay:0.25s;
    }
  }
  &:hover{
    img{
      @include media-query($medium-up){
        @include transform(scale(1.1));
      }
    }
    .bg-des{
      &:before{
        width: 100%;
        right: auto;
        left: 0;
      }
    }
  }
}
figcaption{
  position: relative;
  .bg-des{
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
  }
  .banner-theme &{
    position: absolute;
    width: 90%;
    max-width: 290px;
    align-items: center;
    @include media-query($medium-down){
      width: 100%;
      transform: translateY(0) !important;
      min-width: 100%;
      max-width:100% !important;
      .btn-underline-white{
        border-bottom-color: var(--g-cta-button) !important;
        color: var(--g-cta-button) !important;
      }
    }
  }
  .banner-theme-top &{
    top:40px;
    @include media-query($medium-down){
      top:4%;
    }
  }
  .banner-theme-mid &{
    top:50%;
    transform: translateY(-50%);
  }
  .banner-theme-bottom &{
    bottom:40px;
    @include media-query($medium-down){
      bottom:4%;
    }
  }
  .banner-theme-left &{
    left:40px;
    @include media-query($medium-down){
      left:4%;
    }
  }
  .banner-theme-center &{
    left:50%;
    margin-left:-145px;
    @include media-query($medium-down){
      transform: translateX(-50%)!important;
      margin-left:0;
    }
  }
  .banner-theme-right &{
    right:40px;
    @include media-query($medium-down){
      right:4%;
    }
  }
}


