                    data-src="{{ img_url }}"
                    data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
                    data-aspectratio="{{ section.settings.banner_right1.aspect_ratio }}"
                    data-sizes="auto"
                    alt="{{ section.settings.banner_right1.alt | escape }}">
          {%- endif -%}
          <div class="position-absolute sbanner-text p-lg-5 p-4">
            {{section.settings.banner_right_text_1}}
          </div>
          <a class="sbanner-overlay position-absolute" href="{{section.settings.banner_right_link_1| default:"#"}}"></a>
        </div>
        <div class="w-100 banner-bottom rounded overflow-hidden sbanner position-relative " >
          {%- if section.settings.banner_right2 == blank -%}
          {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
            {%- else -%}
              {% assign img_url = section.settings.banner_right2 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
              <img class="image-content__image lazyload"
