        <div class="w-100 rounded overflow-hidden sbanner position-relative ">
          {%- if section.settings.banner_right1 == blank -%}
            {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
              {%- else -%}
                {% assign img_url = section.settings.banner_right1 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
