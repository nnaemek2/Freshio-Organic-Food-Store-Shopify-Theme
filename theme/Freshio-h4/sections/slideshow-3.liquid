                {{ block.settings.slide_toptitle }}
              </p>
              {%- endunless -%}
              {%- unless block.settings.slide_title == blank -%}
              <h2 class="h1 mega-title slideshow__title slideshow__title--mobile font-family-2 mb-4"  style="color:{{block.settings.slide_title_color}}">
                {{ block.settings.slide_title }}
              </h2>
              {%- endunless -%}
              {%- unless block.settings.subheading == blank -%}
              <div class="mega-subtitle slideshow__subtitle slideshow__subtitle--mobile mb-3" style="color:{{block.settings.subheading_color}}">
                {{ block.settings.subheading }} 
              </div>
              {%- endunless -%}
    
              {%- if show_link_button -%}
              <style>
              .slideshow__btn{{block.id}}{
                  color:{{block.settings.color_button}};
                  border: 1px solid {{block.settings.solid_button}};
                  background-color:{{block.settings.bg_button}};
                }
                .slideshow__btn{{block.id}}:hover{
                  color:{{block.settings.color_button_hover}};
                  border-color:{{block.settings.solid_button_hover}};
                  background-color:{{block.settings.bg_button_hover}};
                }
              </style>
              <a href="{{ block.settings.button_link }}" class="btn gradient-theme slideshow__btn slideshow__btn--mobile slideshow__btn{{block.id}} {%if block.settings.stylebtns == 'default' %} btn-theme {% elsif block.settings.stylebtns == 'underline' %}  btn-underline  {% endif %}">
                <span>{{ block.settings.button_label  }}</span>
              </a> 
              {%- endif -%}
            </div>
          </div>
          {%- endif -%}
          {%- endfor -%}
        </div>
      {%- endunless -%}
      {%- endif -%}
      {%- if section.blocks.size == 0 -%}
      <div class="placeholder-noblocks">
        {{ 'homepage.onboarding.no_content' | t }}
      </div>
      {%- endif -%}
