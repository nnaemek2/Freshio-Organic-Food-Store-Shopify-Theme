                  data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
                  data-aspectratio="{{ section.settings.banner_right2.aspect_ratio }}"
                  data-sizes="auto"
                  alt="{{ section.settings.banner_right2.alt | escape }}">
                
        {%- endif -%}
        <div class="position-absolute sbanner-text p-lg-5 p-4 d-flex align-items-center justify-content-center flex-column">
          {{section.settings.banner_right_text_2}}
        </div>
        <a class="sbanner-overlay position-absolute" href="{{section.settings.banner_right_link_2| default:"#"}}"></a>
        </div>
