<!-- header-3.liquid -->
<header class="site-header page-element is-moved-by-drawer {% if section.settings.enable_sticky %}site-header--sticky{% endif %} {% if section.settings.enable_transparent %} site-header--transparent {% if section.settings.enable_white %}site-header-white {% endif %} {% endif %} {% if section.settings.enable_dropdownfull %}site-header--full{% endif %} {% if template contains 'index' %} index-header {% endif %}" role="banner" data-section-id="{{ section.id }}" data-section-type="header">
 
  <div class="header-top">
    {%- if section.settings.enabletopbar -%}
      <div class="header-head py-2">
        <div class="container-fluid">
          <div class="row">
            {%- for block in section.blocks -%}
            {%- case block.settings.width -%}
            {%- when '25%' -%}
            {%- assign block_width = 'col-lg-3 col-md-6' -%}
            {%- when '33%' -%}
            {%- assign block_width = 'col-lg-4 col-md-6' -%}
            {%- when '50%' -%}
            {%- assign block_width = 'col-lg-6 col-md-6' -%}
            {%- when '66%' -%}
            {%- assign block_width = 'col-lg-8 col-md-12' -%}
            {%- when '75%' -%}
            {%- assign block_width = 'col-lg-9 col-md-12' -%}
            {%- when '100%' -%}
            {%- assign block_width = 'col-12' -%}
            {%- endcase -%}
          
            <div class="{{block_width}}">
              {%- case block.type -%}
              {%- when 'html' -%}
                {% if block.settings.text_head != blank %}
                  <div class="text-sm-{{block.settings.align_head}} text-center">
                    {{ block.settings.text_head}}
                  </div>
                {% endif %}
              {%- when 'custom' -%}
                <div class="text-{{block.settings.align_head_social}} d-none d-lg-block">
                  <ul class="list-inline social-icons">
                    {%- if settings.social_facebook_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_facebook_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Facebook' }}">
                        {%- include 'icon-facebook' -%}
                        <span >Fb</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_twitter_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_twitter_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Twitter' }}">
                        {%- include 'icon-twitter' -%}
                        <span >Tw</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_pinterest_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_pinterest_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Pinterest' }}">
                        {%- include 'icon-pinterest' -%}
                        <span>Pin</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_instagram_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_instagram_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Instagram' }}">
                        {%- include 'icon-instagram' -%}
                        <span >Ins</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_tumblr_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_tumblr_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Tumblr' }}">
                        {%- include 'icon-tumblr' -%}
                        <span >Tum</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_snapchat_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_snapchat_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Snapchat' }}">
                        {%- include 'icon-snapchat' -%}
                        <span >Snap</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_youtube_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_youtube_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'YouTube' }}">
                        {%- include 'icon-youtube' -%}
                        <span class="icon__fallback-text">You</span>
                      </a>
                    </li>
                    {%- endif -%}
                    {%- if settings.social_vimeo_link != blank -%}
                    <li class="list-inline-item">
                      <a href="{{ settings.social_vimeo_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Vimeo' }}">
                        {%- include 'icon-vimeo' -%}
                        <span >Vim</span>
                      </a>
                    </li>
                    {%- endif -%}
                  </ul>
                </div>
              {%- endcase -%}
            </div>
            {%- endfor -%}
          </div>
        </div>
      </div>
    {%- endif -%}
    <div class="header-bottom">
      <div class="container-fluid">
        <div class="row row-header pt-3 pb-3 pb-lg-0">
          <div class="d-block d-lg-none toggle-menu-mobile col-3 col-md align-items-center d-flex">
            {%- include 'button-toggle-menu-mobile' -%}
          </div>
          <div class="col-md-4 col-3 col-sm-3 header-top__left d-none d-lg-flex">
            {%- if section.settings.enablesidebar -%}
              {%- include 'sidebar-option' -%}
            {%- endif -%}
        </div>
          <div class="col-sm-12 col-md-4 col-6 d-flex align--center justify-content-center col-logo {% if section.settings.animation_logo %} animation-logo{% endif %}">
            {%- include 'site-logo' -%}
          </div>
          <div class="col-sm-3 col-md-4 col-3 header-top__right">
            <!-- header-3 | login -->
            {% if shop.customer_accounts_optional or shop.customer_accounts_enabled %}
            {% if customer  %}
            <div class="header--acount d-none d-lg-inline-block ">
              <a href="/account"  class="site-header__link site-account ">
                <span class="account-text">{{'customer.account.title' | t}}</span>
              </a>
              <a href="/account/logout" class="site-header__link site-logout mr-3">
                <span class="logout-text">{{'layout.customer.log_out' | t}}</span>
              </a>
            </div>
            {% else %}
            {% if template != 'customers/login' %} {% comment DOES NOT APPEAR ON LOGIN PAGE - AVOID CONFLICT %} {% endcomment %}
            <div class="header--acount d-none d-lg-inline-block font-family-1">
              <a href="javascript:void(0)" data-toggle="modal" data-target="#jsLogin" class="site-header__link site-account js-open-login">
                
                <span class="account-text">{{'customer.login.sign_in' | t}}</span>
              </a> /
              <a href="/account/register" class="site-header__link site-register mr-3">
                <span class="register-text">{{'customer.register.title' | t}}</span>
              </a>
            </div>
            {% endif %}
            {% endif %}
            {% endif %}  
            {%- if section.settings.currencies -%}
            <div class="mr-4 d-none d-lg-block">
              {%- include 'block-currencies' -%}
            </div>
            {%- endif -%}
            <!-- header-3 | search -->
            <div class="site-header__search-wrap">
              <div class="overlay" onclick="toggleClassSearch()"></div>
              <form id="form-search-header" action="/search" method="get" class="site-header__search js-ajaxsearch" role="search">
                <div class="container py-5">
                  <div class="row">
                    <div class="col-12">
                      
                      <input type="hidden" name="type" value="product">
                      <div class="d-flex align-items-center mb-2">
                        <label for="SiteNavSearch" class="label-search mr-auto mb-0 text-uppercase text-muted">{{ 'general.search.titleplace' | t }}</label>
                        <a href="#" onclick="toggleClassSearch()">{%- include 'icon-close' -%}</a>
                      </div>
                      
                      <div class="d-flex align-items-center pb-lg-2 border-bottom">
                        <input onfocus="this.value=''" type="search" name="q" id="SiteNavSearch" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" autocomplete="off" class="site-header__search-input mr-auto">
                        <button type="submit" class="text-link site-header__link site-header__search-submit">
                          {%- include 'icon-search' -%}
                        </button>
                      </div>
                      <div class="search-results"></div>
                    </div>
                  </div>
                </div>
              </form>
              <button onclick="toggleClassSearch()" class="text-link site-header__link site-header__search-pop">
                {%- include 'icon-search' -%}
              </button>
              
            </div>
            {%- unless section.settings.sidebar_heart -%}
              <div class="wishlist-right">
                {%- include 'block-wishlist' -%}
              </div>
            {% endunless %}
            {%- include 'block-cart' -%}
          </div>
          </div>
          <div class="row">
            <div class="col-md-12 col d-none d-lg-block text-center col-menu">
              {%- include 'block-menu' -%}
            </div>
          </div>
      </div>
    </div>
      
    </div>
  </div>
</header>
{%- if template != 'customers/login' -%}
{% include 'popup-login' %}
{%- endif -%}
