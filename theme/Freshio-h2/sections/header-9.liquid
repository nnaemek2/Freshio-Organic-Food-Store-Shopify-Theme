<!-- header-9.liquid -->
{%- if section.settings.enabletopbar -%}
  <div id="topbar">
    <div class="container">
      <div class="row">
      {%- if section.settings.topbartext != blank -%}
        <div class="col-lg-6 col-left justify-content-center justify-content-lg-start">
          {{section.settings.topbartext}}
        </div>
      {%- endif -%}
      {%- if section.settings.text_topbar != blank -%}
        <div class="col-lg-6 col-right justify-content-lg-end d-none d-lg-flex">
          <div class="html-topbar topbar-block">
          {{ section.settings.text_topbar}}
          </div>
        </div>
        {%- endif -%}
      </div>
    </div>
  </div>
{%- endif -%}
<header class="site-header page-element is-moved-by-drawer {% if section.settings.enable_sticky %}site-header--sticky{% endif %}  {% if section.settings.enable_dropdownfull %}site-header--full{% endif %} {% if template contains 'index' %} index-header {% endif %}" role="banner" data-section-id="{{ section.id }}" data-section-type="header">
  
  <div class="header-main">
    <div class="container">
      <div class="row no-gutters align-items-center">
        <div class="d-block d-lg-none toggle-menu-mobile col-3">
          {%- include 'button-toggle-menu-mobile' -%}
        </div>
        <div class="d-flex align--center  col-logo justify-content-center justify-content-lg-start col-lg-3">
          {%- include 'site-logo' -%}
        </div>
       
        <div class="col-lg-6 col-menu d-none d-lg-block">
            {%- include 'block-menu' -%}
        </div>
       
        <div class="ml-auto header-bottom__right col-lg-3 col-3">
            {%- if section.settings.e_search -%}
            <div class="site-header__search-wrap mr-3 d-none d-lg-block">
                <div class="overlay" onclick="toggleClassSearch()"></div>
                <form id="form-search-header" action="/search" method="get" class="site-header__search js-ajaxsearch" role="search">
                  <div class="container py-5">
                    <div class="row">
                      <div class="col-12">
                        
                        <input type="hidden" name="type" value="product">
                        <div class="d-flex align-items-center mb-3">
                          <label for="SiteNavSearch" class="label-search mr-auto mb-0 text-uppercase text-muted small">{{ 'general.search.titleplace' | t }}</label>
                          <a href="#" onclick="toggleClassSearch()">{%- include 'icon-close' -%}</a>
                        </div>
                        
                        <div class="d-flex align-items-center pb-lg-2">
                          <input onfocus="this.value=''" type="search" name="q" id="SiteNavSearch" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" autocomplete="off" class="site-header__search-input mr-auto font-family-1">
                          <button type="submit" class="text-link site-header__link site-header__search-submit">
                            {%- include 'icon-search' -%}
                          </button>
                        </div>
                        <div class="search-results p-5"></div>
                      </div>
                    </div>
                  </div>
                </form>
                <button onclick="toggleClassSearch()" class="text-link site-header__link site-header__search-pop">
                  {%- include 'icon-search' -%}
                </button>
              </div>
            {% comment %}
            <form id="form-search-header" action="/search" method="get" class="site-header__search js-ajaxsearch" role="search">
                    <input type="hidden" name="type" value="product">
                    <div class="d-flex align-items-center">
                      <input onfocus="this.value=''" type="search" name="q" id="SiteNavSearch" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" autocomplete="off" class="site-header__search-input mr-auto font-family-1">
                      {%- include 'icon-search' -%}
                      <button type="submit" class="text-link site-header__link site-header__search-submit">
                        {{ 'general.search.submit' | t }}
                      </button>
                    </div>
                    <div class="search-results shadow"></div>
            </form>
            {% endcomment %}
            {%- endif -%}
          {%- if section.settings.e_login -%}
            {%- if shop.customer_accounts_optional or shop.customer_accounts_enabled -%}
              {%- if customer -%}
              <div class="header--acount d-none d-lg-inline-block">
                <a href="/account" class="site-header__link site-account pr-2">
                  <span class="account-text">{{'customer.account.title' | t}}</span>
                </a>
                <a href="/account/logout" class="pl-2 site-header__link site-logout mr-3" >
                  <span class="logout-text">{{'layout.customer.log_out' | t}}</span>
                </a>
              </div>
              {%- else -%}
              <div class="header--acount d-none d-lg-inline-block mr-3" >
                <a  class="site-header__link site-account pr-2 site-login" href="javascript:void(0)" data-toggle="modal" data-target="#jsLogin">
                  {% render 'icon-user'%}
                </a>
              </div>
              {%- endif -%}
            {%- endif -%}
          {%- endif -%}
          {%- if section.settings.e_wishlist -%}
            <div class="wishlist-right">
              {%- include 'block-wishlist' -%}
            </div>
          {% endif %}
          {%- include 'block-cart' -%}
        </div>
        
        </div>
      </div>
    </div>
  </div>
</header>
{%- if template != 'customers/login' -%}
{% include 'popup-login' %}
{%- endif -%}
